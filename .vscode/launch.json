{
    "version": "0.2.0",
    "configurations": [
        

        {
            "name": "Python Debugger: Current File with Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": "${command:pickArgs}"
        },

        {
            "name": "Python: Debug generate.py",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/TGAN/Generator/generate.py",
            "args": [
                "--csv_dir='../generated/generated_alleles_fixed_ratios_base_template_500_5000_improved/epgs' --output_dir='../../generated_epgs/fixed_ratios_base_template_500_5000_scaled' --batch_size=64 --epg_shape 4000 5000"
            ]
        }
    ]
}